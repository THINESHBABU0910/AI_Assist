      "api_endpoints": {
        "id": "api_endpoints",
        "applications": [
          {
            "id": "applications_endpoint",
            "method": "GET",
            "path": "/api/applications",
            "description": "Fetch available blueprint applications",
            "query_params": [
              {
                "id": "param_category",
                "name": "category",
                "type": "string",
                "description": "Filter by category (all, Machine Learning, GenAI, etc.)"
              },
              {
                "id": "param_search",
                "name": "search",
                "type": "string",
                "description": "Search term for filtering applications"
              },
              {
                "id": "param_page",
                "name": "page",
                "type": "integer",
                "description": "Page number for pagination"
              },
              {
                "id": "param_pageSize",
                "name": "pageSize",
                "type": "integer",
                "description": "Number of items per page"
              }
            ],
            "response": {
              "id": "response_applications",
              "applications": "array",
              "total": "integer"
            }
          }
        ],
        "deployments": [
          {
            "id": "deployments_endpoint",
            "method": "GET",
            "path": "/api/deployments",
            "description": "Fetch deployment status with pagination",
            "query_params": [
              {
                "id": "param_limit",
                "name": "limit",
                "type": "integer",
                "description": "Number of deployments to fetch"
              },
              {
                "id": "param_last_id",
                "name": "last_id",
                "type": "string",
                "description": "Cursor for pagination"
              }
            ],
            "response": {
              "id": "response_deployments",
              "deployments": "array",
              "next_cursor": "string"
            }
          },
          {
            "id": "deployment_details_endpoint",
            "method": "GET",
            "path": "/api/deployment/{deployment_id}",
            "description": "Fetch specific deployment details",
            "path_params": [
              {
                "id": "param_deployment_id",
                "name": "deployment_id",
                "type": "string",
                "description": "Unique deployment identifier"
              }
            ],
            "response": {
              "id": "response_deployment",
              "deployment": "object"
            }
          }
        ],
        "chat": [
          {
            "id": "chat_endpoint",
            "method": "POST",
            "path": "/api/chat/ask",
            "description": "AI chat assistance endpoint",
            "body_params": [
              {
                "id": "param_question",
                "name": "question",
                "type": "string",
                "description": "User question for AI assistance"
              }
            ],
            "response": {
              "id": "response_chat",
              "answer": "string",
              "conversation_id": "string",
              "total_tokens": "integer",
              "cost": "number"
            }
          }
        ]
