 {
 "appfoundry": {
      "id": "appfoundry",
      "name": "Appfoundry",
      "version": "0.1.0",
      "description": "AI-powered application deployment platform with blueprint management",
      "route": "/appfoundry",
      "framework": "Next.js",
      "language": "TypeScript",
      "styling": "Tailwind CSS",
      "ui_components": "Radix UI",
      "port": 3003,
      "production_port": 3003,
      "technology_stack": {
        "id": "technology_stack",
        "frontend": {
          "id": "frontend",
          "framework": "Next.js 13.5.1",
          "language": "TypeScript 5.2.2",
          "styling": "Tailwind CSS 3.3.3",
          "ui_components": "Radix UI",
          "state_management": "React Hooks",
          "form_handling": "React Hook Form",
          "validation": "Zod"
        },
        "backend": {
          "id": "backend",
          "chat_backend": "FastAPI (Python)",
          "ai_platform": "Sify AI Platform",
          "api_base_url": "https://copilot-dev.sifymdp.digital/v1"
        },
        "development": {
          "id": "development",
          "package_manager": "npm",
          "development_server": "Next.js dev server (port 3003)",
          "chat_backend_server": "uvicorn (port 8000)",
          "linting": "ESLint",
          "formatting": "PostCSS"
        }
      },
      "ui_automation": {
        "id": "ui_automation",
        "selectors": [
          {
            "id": "deploy-button",
            "selector": "button:contains('Deploy')",
            "action": "click",
            "description": "Deploy application button"
          },
          {
            "id": "details-button",
            "selector": "button:contains('Details')",
            "action": "click",
            "description": "View application details"
          },
          {
            "id": "search-input",
            "selector": "input[placeholder*='search']",
            "action": "input",
            "description": "Search applications input in header search bar"
          },
          {
            "id": "chat-input",
            "selector": "textarea[placeholder*='message']",
            "action": "input",
            "description": "Chat message input"
          },
          {
            "id": "home-link",
            "selector": "a[href='/']",
            "action": "click",
            "description": "App Foundry logo/home link"
          },
          {
            "id": "my-blueprints-nav",
            "selector": "a[href='/my-blueprints']",
            "action": "click",
            "description": "My Blueprints navigation link"
          },
          {
            "id": "cloud-assist-toggle",
            "selector": "button[title='Open Cloud Assist']",
            "action": "click",
            "description": "Toggle CloudAssist sidebar"
          },
          {
            "id": "assist-console-toggle",
            "selector": "button[title='Open Center Chat']",
            "action": "click",
            "description": "Toggle AssistConsole floating chat"
          },
          {
            "id": "deploy-modal-confirm",
            "selector": "button:contains('Confirm')",
            "action": "click",
            "description": "Confirm deployment in modal"
          }
        ]
      },
      "workflows": {
        "id": "workflows",
        "deploy_application": {
          "id": "deploy_application",
          "steps": [
            {
              "id": "deploy_step_1",
              "action": "click",
              "target": "button:contains('Deploy')"
            },
            {
              "id": "deploy_step_2",
              "action": "wait",
              "target": "div[role='dialog']"
            },
            {
              "id": "deploy_step_3",
              "action": "click",
              "target": "button:contains('Confirm')"
            }
          ]
        },
        "chat_assistance": {
          "id": "chat_assistance",
          "steps": [
            {
              "id": "chat_step_1",
              "action": "click",
              "target": "button[title='Open Cloud Assist']"
            },
            {
              "id": "chat_step_2",
              "action": "input",
              "target": "textarea[placeholder*='message']"
            },
            {
              "id": "chat_step_3",
              "action": "click",
              "target": "button[aria-label='Send']"
            }
          ]
        }
      },
      "pages": [
        {
          "id": "home",
          "route": "/appfoundry",
          "title": "Home",
          "summary": "Landing page with blueprint overview and AI assistance",
          "description": "Main dashboard showing available blueprints, deployment status, and AI chat assistance",
                  "layout": {
          "id": "home_layout",
          "containers": [
            "header",
            "sidebar",
            "main-content",
            "cloud-assist",
            "assist-console"
          ],
          "search_bar": {
            "id": "header_search_bar",
            "location": "header",
            "component": "SearchBar",
            "placeholder": "Search blueprints...",
            "keyboard_shortcut": "Ctrl+K"
          }
        },
          "components": [
            "Header",
            "Sidebar",
            "BlueprintsSection",
            "CloudAssist",
            "AssistConsole",
            "SearchBar"
          ],
          "endpoints": [
            {
              "id": "endpoint_applications",
              "method": "GET",
              "path": "/api/applications",
              "description": "Fetch available blueprint applications"
            },
            {
              "id": "endpoint_deployments",
              "method": "GET",
              "path": "/api/deployments",
              "description": "Fetch deployment status"
            },
            {
              "id": "endpoint_chat",
              "method": "POST",
              "path": "/api/chat/ask",
              "description": "AI chat assistance"
            }
          ],
          "actions": [
            {
              "id": "search_blueprints",
              "name": "Search Blueprints",
              "description": "Search through available blueprint applications using the search bar in the header",
              "api_call": {
                "id": "api_search_blueprints",
                "method": "GET",
                "path": "/api/applications",
                "params": [
                  "search",
                  "category",
                  "page",
                  "pageSize"
                ]
              },
              "ui_component": "SearchBar",
              "features": [
                "Debounced search with 300ms delay",
                "Keyboard shortcut (Ctrl+K) to focus",
                "Clear button when search has content",
                "Real-time filtering of application cards",
                "Search placeholder: 'Search blueprints...'"
              ]
            },
            {
              "id": "deploy_application",
              "name": "Deploy Application",
              "description": "Deploy a selected blueprint",
              "api_call": {
                "id": "api_deploy_application",
                "method": "POST",
                "path": "/api/deployments",
                "params": [
                  "blueprint_id",
                  "deployment_config"
                ]
              }
            },
            {
              "id": "chat_assistance",
              "name": "AI Chat Assistance",
              "description": "Get AI-powered help and guidance",
              "api_call": {
                "id": "api_chat_assistance",
                "method": "POST",
                "path": "/api/chat/ask",
                "params": [
                  "question"
                ]
              }
            }
          ]
        },
        {
          "id": "my-blueprints",
          "route": "/my-blueprints",
          "title": "My Blueprints",
          "summary": "Manage and view deployed applications",
          "description": "Page to view and manage all deployed blueprint applications with status tracking",
          "layout": {
            "id": "my_blueprints_layout",
            "containers": [
              "header",
              "deployment-list",
              "search-filter",
              "pagination"
            ]
          },
          "components": [
            "Header",
            "DeploymentCard",
            "TableRow",
            "StatusBadge"
          ],
          "endpoints": [
            {
              "id": "endpoint_user_deployments",
              "method": "GET",
              "path": "/api/deployments",
              "description": "Fetch user deployments with pagination"
            }
          ],
          "actions": [
            {
              "id": "view_deployments",
              "name": "View Deployments",
              "description": "List all user deployments",
              "api_call": {
                "id": "api_view_deployments",
                "method": "GET",
                "path": "/api/deployments",
                "params": [
                  "limit",
                  "last_id"
                ]
              }
            },
            {
              "id": "search_deployments",
              "name": "Search Deployments",
              "description": "Search through deployments",
              "api_call": {
                "id": "api_search_deployments",
                "method": "GET",
                "path": "/api/deployments",
                "params": [
                  "search",
                  "status",
                  "limit"
                ]
              }
            },
            {
              "id": "get_deployment_details",
              "name": "Get Deployment Details",
              "description": "Fetch specific deployment information",
              "api_call": {
                "id": "api_get_deployment_details",
                "method": "GET",
                "path": "/api/deployment/{deployment_id}",
                "params": [
                  "deployment_id"
                ]
              }
            }
          ]
        },
        {
          "id": "details",
          "route": "/details/{id}",
          "title": "Application Details",
          "summary": "View detailed blueprint information",
          "description": "Detailed view of a blueprint with architecture information and deployment options",
          "layout": {
            "id": "details_layout",
            "containers": [
              "header",
              "hero-section",
              "overview-content",
              "architecture-gallery",
              "deploy-modal"
            ]
          },
          "components": [
            "HeaderClient",
            "ArchImage",
            "MarkdownRenderer",
            "ArchitectureGallery",
            "DeployButtonWithModal",
            "DeployModal"
          ],
          "endpoints": [
            {
              "id": "endpoint_application_details",
              "method": "GET",
              "path": "/api/applications/{id}",
              "description": "Fetch specific application details"
            }
          ],
          "actions": [
            {
              "id": "deploy_blueprint",
              "name": "Deploy Blueprint",
              "description": "Deploy the selected blueprint",
              "api_call": {
                "id": "api_deploy_blueprint",
                "method": "POST",
                "path": "/api/deployments",
                "params": [
                  "blueprint_id",
                  "deployment_config"
                ]
              }
            },
            {
              "id": "view_github",
              "name": "View on GitHub",
              "description": "Open blueprint repository on GitHub",
              "api_call": {
                "id": "api_view_github",
                "method": "GET",
                "path": "external",
                "params": [
                  "github_url"
                ]
              }
            }
          ]
        }
      ],
