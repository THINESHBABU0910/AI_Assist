            "id": "filter_action_1",
            "action": "click",
            "target": "sidebar filters",
            "description": "Filter models by capability, provider, or type"
          },
          {
            "id": "filter_action_2",
            "action": "type",
            "target": "search input",
            "description": "Search models by name or description"
          },
          {
            "id": "filter_action_3",
            "action": "click",
            "target": "filter badges X button",
            "description": "Remove active filters"
          }
        ],
        "model_interaction": [
          {
            "id": "model_action_1",
            "action": "click",
