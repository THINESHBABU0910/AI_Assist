            "id": "playground_action_2",
            "action": "type",
            "target": "chat input",
            "description": "Send message to model"
          },
          {
            "id": "playground_action_3",
            "action": "click",
            "target": "generate code button",
            "description": "Generate code with model"
          },
          {
            "id": "playground_action_4",
            "action": "click",
